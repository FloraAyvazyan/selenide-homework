<suite name="SelenideTests2Suite" thread-count="1" parallel="tests">
    <listeners>
        <listener class-name="Listeners.ReportListener" />
        <listener class-name="Listeners.SuiteListener" />
        <listener class-name="Listeners.TestListener" />
    </listeners>
    <test name="ChromeTest">
        <parameter name="browserType" value="chrome"/>
        <classes>
            <class name="SelenideTests2"/>
        </classes>
    </test>
    <test name="EdgeTest">
        <parameter name="browserType" value="edge"/>
        <classes>
            <class name="SelenideTests2"/>
        </classes>
    </test>
    <test name="FirefoxTest">
        <parameter name="browserType" value="firefox"/>
        <classes>
            <class name="SelenideTests2"/>
        </classes>
    </test>
</suite>